{
    "name": "elegasoft/laravel-storage-route",
    "description": "Provides a route to handle the storage path as an alternative to the default Laravel symlink",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Cook",
            "email": "jason@elegasoft.com"
        }
    ],
    "autoload": {
        "psr-4": {
